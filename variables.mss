/**
 * COLORS
 * ============================================================================
 */

@white:             #fefefe;
@black:             #0f0f0f;

@map:               #efe5d9;
@landcover:         #b2c58a;

@landuseGreen:      #b2c58a;
@landuseRed:        #E6BAA8;

@building:         #efe5d9;
